<?xml version="1.0" encoding="utf-8"?>
<AbsoluteLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="true">

    <Button
        android:id="@+id/Cuidador"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_x="135dp"
        android:layout_y="454dp"
        android:background="@color/design_default_color_background"
        android:text="Cuidador"
        android:textColor="@android:color/holo_green_dark" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="38dp"
        android:layout_x="129dp"
        android:layout_y="281dp"
        android:fontFamily="serif"
        android:text="Pata@ventura"
        android:textColor="@android:color/holo_green_dark"
        android:textSize="24sp" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="112dp"
        android:layout_y="59dp"
        app:srcCompat="@drawable/logo" />

    <Button
        android:id="@+id/guarderia"
        android:layout_width="206dp"
        android:layout_height="wrap_content"
        android:layout_x="134dp"
        android:layout_y="543dp"
        android:background="@color/design_default_color_background"
        android:text="Propietario"
        android:textColor="@android:color/holo_green_dark" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="64dp"
        android:layout_height="63dp"
        android:layout_x="49dp"
        android:layout_y="538dp"
        app:srcCompat="@drawable/iconopropietario" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="63dp"
        android:layout_height="68dp"
        android:layout_x="47dp"
        android:layout_y="444dp"
        app:srcCompat="@drawable/logocuidador" />
</AbsoluteLayout>